    <div class="panel top">
      <div class="rank">Points</div>
      <div class="suit"><%=data[2]%></div>
      <div class="icon"><svg>
        <defs>
          <pattern id="img1" patternUnits="userSpaceOnUse" width="21" height="21">
            <image xlink:href="../img/caragato.jpg" width="21" height="21" />
          </pattern>
        </defs>
        <rect width="21" height="21" fill="url(#img1)" />
        </svg></div>
    </div>
    <div class="content #{className}">
      <div><img class="song" src="../img/<%=data[4]%>"></div>
    </div>
   
    <div class="panel bottom">
      <div class="rank">Points</div>
      <div class="suit"><%=data[2]%></div>
      <div class="icon"><svg>
          <defs>
            <pattern id="img1" patternUnits="userSpaceOnUse" width="21" height="21">
